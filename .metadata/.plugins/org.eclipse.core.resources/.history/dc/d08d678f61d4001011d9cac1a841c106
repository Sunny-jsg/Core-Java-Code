package in.ramana;

public class Q18 {
	public static void main(String[] args) {
		int[] arr = { 1, 2, 3, 2, 4, 1 };
		for(int i=0;i<=arr.length-1;i++) {
			for(int j=0;j<arr.length-1-i;j++) {
				if(arr[j]>arr[j-1]) {
					int temp =arr[j-1];
					arr[j-1] =arr[j];
					arr[j]= temp;
				}
			}
			for(int s : arr) {
				System.out.print(s + " ");
			}
			
		}
		
		
		
		
	}

}
