package in.sunny;

public class SecondLargestNumber {
	public static void main(String[] args) {
		int [] arr = {4,5,3,2,6,1};
		int max =Integer.MIN_VALUE;
		int secondMax =Integer.MIN_VALUE;
		for(int i=0;i<arr.length;i++) {
			secondMax=max;
			if(arr[i]>max) {
				max =arr[i];
			}
		}
		System.out.println(secondMax);
	}

}
