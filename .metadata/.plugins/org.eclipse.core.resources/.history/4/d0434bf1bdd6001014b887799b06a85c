package in.sunny;

import java.util.Arrays;

public class RotateArray {
	public static void main(String[] args) {
		int [] arr = {1,2,3,4,5,6};
		int last = arr[arr.length - 1];
		for (int k = arr.length - 1; k > 0; k--) {
		    arr[k] = arr[k - 1];
		}
		arr[0] = last;

		System.out.println(Arrays.toString(arr));
	}

}
