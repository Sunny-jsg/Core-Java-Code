package in.sunny;

public class SumOfEvenIndex {
	public static void main(String[] args) {
		int[] arr = { 1, 2, 3, 4, 5, 6, 3, 2, 1, 4, 9 };
		evenIndex(arr);
	}
	
	
	public static void evenIndex(int [] arr) {
		int sum =0;
		for(int i=0;i<arr.length;i++) {
			if(i%2 ==0) {
				sum +=arr[i];
			}
		}
		System.out.println("Sum Of Even Index : " + sum);
	}

}
