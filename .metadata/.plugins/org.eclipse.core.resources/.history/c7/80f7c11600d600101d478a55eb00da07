package in.sunny;

public class ReverseString {
	public static void main(String[] args) {
		String s1 ="Hello";
		String s = reverse(s1);
		System.out.println(s);
	}
	public static String reverse(String s) {
		String s2 ="";
		char[] ch = s.toCharArray();
		for(int i=ch.length-1;i>=0;i--) {
			s2 += ch[i];
		}
		return s2;
	}
	
	

}
