package in.ramana;

public class Q25 {
	public static void main(String[] args) {
		int[][] arr = { { 2, 3, 4, 1 }, { 2, 3, 5, 6, 7, 8, 9 } };
		int sum = 0;
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr[i].length; j++) {
				sum += arr[i][j];
			}
			System.out.print(sum+ " ");

		}
		
	}

}
