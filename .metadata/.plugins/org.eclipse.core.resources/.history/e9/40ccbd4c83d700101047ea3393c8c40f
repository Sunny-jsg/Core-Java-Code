package in.demo;

import java.util.ArrayList;

public class Student {
	int id;
	String sname;
	String marks;
	String gender;
	Long phno;

	public Student(int id, String sname, String marks, String gender, Long phno) {
		this.id = id;
		this.sname = sname;
		this.marks = marks;
		this.gender = gender;
		this.phno = phno;
	}

	public static void main(String[] args) {
		ArrayList al = new ArrayList<>();
		Student s1 = new Student(1, "Rahul", "85", "Male", 9876543210L);
		Student s2 = new Student(2, "Priya", "90", "Female", 9876543211L);
		Student s3 = new Student(3, "Karan", "78", "Male", 9876543212L);
		Student s4 = new Student(4, "Neha", "88", "Female", 9876543213L);
		Student s5 = new Student(5, "Amit", "92", "Male", 9876543214L);
		Student s6 = new Student(6, "Sonia", "80", "Female", 9876543215L);
		Student s7 = new Student(7, "Deepak", "74", "Male", 9876543216L);
		Student s8 = new Student(8, "Meena", "89", "Female", 9876543217L);
		Student s9 = new Student(9, "Ravi", "95", "Male", 9876543218L);
		Student s10 = new Student(10, "Anjali", "84", "Female", 9876543219L);

		al.add(s1);
		al.add(s2);
		al.add(s3);
		al.add(s4);
		al.add(s5);
		al.add(s6);
		al.add(s7);
		al.add(s8);
		al.add(s9);
		al.add(s10);
		System.out.println(al);
		
	}

	@Override
	public String toString() {
		return "Student [id=" + id + ", sname=" + sname + ", marks=" + marks + ", gender=" + gender + ", phno=" + phno
				+ "]";
	}
}
