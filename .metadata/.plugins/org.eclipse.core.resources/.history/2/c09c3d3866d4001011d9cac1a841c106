package in.ramana;

public class Q19 {
	public static void main(String[] args) {
		int[] arr = { 1,2,3,4,5 };
		int min =90000;
		int secondMin =90000;
		for(int i=0;i<=arr.length-1;i++) {
			if(min > arr[i]) {
				secondMin =min;
				min =arr[i];
			} else if (arr[i] < secondMin && arr[i] != min) {
                secondMin = arr[i];
		}
		
	}
		System.out.println(secondMin);

	}
}
