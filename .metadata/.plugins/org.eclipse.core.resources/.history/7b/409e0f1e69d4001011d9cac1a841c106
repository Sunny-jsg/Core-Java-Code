package in.ramana;

public class Q21 {
	public static void main(String[] args) {
		int [] arr = {23,55,57,93,10,1};
		int sum =0;
		for(int i=0;i<=arr.length-1;i++) {
			while(arr[i] > 0) {
			int d = arr[i]%10;
			sum +=d; 
			arr[i] /=10;
			}
			if(sum >9) {
				sum /=10;
			}
			System.out.println(sum);
			sum=0;
		}
		
	}

}
