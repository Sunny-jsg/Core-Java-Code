package in.sunny;

import java.util.Arrays;

public class RotateArray {
	public static void main(String[] args) {
		int [] arr = {1,2,3,4,5,6};
		int start=arr[0];
		for(int k=0;k<arr.length-1;k++) {
			arr[k] =arr[k+1];
		}
		arr[arr.length - 1] = start;
		System.out.println(Arrays.toString(arr));
	}

}
