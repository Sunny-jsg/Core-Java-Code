package in.sunny;

public class Sum {
	public static void main(String[] args) {
		String s1 ="Sunny123";
		int ans = sum(s1);
		System.out.println(ans);
		
	}
	public static int sum(String s1) {
		int sumdigit =0;
		char[] ch = s1.toCharArray();
		for(char c : ch) {
			if(c > '0' && c<'9') {
				int n =c-'0';
				sumdigit =sumdigit +n;
			}
		}
		return sumdigit;
	}

}
