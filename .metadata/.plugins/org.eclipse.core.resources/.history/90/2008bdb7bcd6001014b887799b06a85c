package in.sunny;

public class RotateArray {
	public static void main(String[] args) {
		int [] arr = {1,2,3,4,5,6};
		int i =0;
		int j = arr.length-1;
		while(i<j) {
			int temp =arr[i];
			arr[i]=arr[j];
			arr[j]=temp;
			i++;
			j--;
		}
		for (int x : arr) {
            System.out.print(x + " ");
        }
		int last = arr[arr.length - 1];
		for (int k = arr.length - 1; k > 0; k--) {
		    arr[k] = arr[k - 1];
		}
		arr[0] = last;

		
	}

}
